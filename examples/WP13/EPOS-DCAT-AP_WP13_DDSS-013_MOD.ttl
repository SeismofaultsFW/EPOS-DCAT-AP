###############################################################################
### Web service operations ###
###############################################################################

<https://www.epos-eu.org/epos-dcat-ap/GeoElectroMagnetism/WP13-DDSS-013/MOD/Operation> a hydra:Operation;
    hydra:method "GET"^^xsd:string;
    hydra:returns "application/json";
    hydra:property[ a hydra:IriTemplate;
        hydra:template "http://mt.bgs.ac.uk/{project}/{survey}/MOD/{project}{survey}_{model}.mod.json"^^xsd:string;
            hydra:mapping[ a hydra:IriTemplateMapping;
                hydra:variable "project"^^xsd:string;
                rdfs:label "Project name";
                rdfs:range "xsd:string" ;
                schema:defaultValue "EPOS";
                hydra:required "true"^^xsd:boolean;
            ];
            hydra:mapping[ a hydra:IriTemplateMapping;
                hydra:variable "survey"^^xsd:string;
                rdfs:label "Survey name";
                rdfs:range "xsd:string" ;
                schema:defaultValue "2019";
                hydra:required "true"^^xsd:boolean;
            ];
            hydra:mapping[ a hydra:IriTemplateMapping;
                hydra:variable "model"^^xsd:string;
                rdfs:label "Model abbreviation";
                rdfs:range "xsd:string" ;
                schema:defaultValue "3D";
                hydra:required "true"^^xsd:boolean;
            ];
    ] ;
.



