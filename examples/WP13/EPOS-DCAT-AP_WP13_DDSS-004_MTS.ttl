###############################################################################
### Web service operations ###
###############################################################################

<https://www.epos-eu.org/epos-dcat-ap/GeoElectroMagnetism/WP13-DDSS-004/MTS/Operation> a hydra:Operation;
    hydra:method "GET"^^xsd:string;
    hydra:returns "application/json";
    hydra:property[ a hydra:IriTemplate;
        hydra:template "http://mt.bgs.ac.uk/{project}/{survey}/TS/{project}{survey}_{site}.ts.json"^^xsd:string;
            hydra:mapping[ a hydra:IriTemplateMapping;
                hydra:variable "StartTime"^^xsd:string;
                rdfs:label "Start time";
                rdfs:range "xsd:string";
                hydra:property "schema:startDate";
                schema:defaultValue "2000-01-01T00:00:00Z";
                hydra:required "false"^^xsd:boolean;
            ];
            hydra:mapping[ a hydra:IriTemplateMapping;
                hydra:variable "StopTime"^^xsd:string;
                rdfs:label "Stop time";
                rdfs:range "xsd:string";
                hydra:property "schema:endDate";
                schema:defaultValue "2019-01-01T00:00:00Z";
                hydra:required "false"^^xsd:boolean;
            ];
            hydra:mapping[ a hydra:IriTemplateMapping;
                hydra:variable "north-latitude"^^xsd:string;
                hydra:property "epos:northernmostLatitude";
                rdfs:label "North latitude";
                rdfs:range "xsd:float";
                schema:defaultValue "90.0";
                schema:minValue "-90.0";
                schema:maxValue "90.0";
                hydra:required "false"^^xsd:boolean;
            ];
            hydra:mapping[ a hydra:IriTemplateMapping;
                hydra:variable "south-latitude"^^xsd:string;
                hydra:property "epos:southernmostLatitude";
                rdfs:label "South latitude";
                rdfs:range "xsd:float";
                schema:defaultValue "-90.0";
                schema:minValue "-90.0";
                schema:maxValue "90.0";
                hydra:required "false"^^xsd:boolean;
            ];
            hydra:mapping[ a hydra:IriTemplateMapping;
                hydra:variable "west-longitude"^^xsd:string;
                hydra:property "epos:westernmostLongitude";
                rdfs:label "West longitude";
                rdfs:range "xsd:float";
                schema:defaultValue "0.0";
                schema:minValue "-180.0";
                schema:maxValue "360.0";
                hydra:required "false"^^xsd:boolean;
            ];
            hydra:mapping[ a hydra:IriTemplateMapping;
                hydra:variable "east-longitude"^^xsd:string;
                hydra:property "epos:easternmostLongitude";
                rdfs:label "East longitude";
                rdfs:range "xsd:float";
                schema:defaultValue "360.0";
                schema:minValue "-180.0";
                schema:maxValue "360.0";
                hydra:required "false"^^xsd:boolean;
            ];
            hydra:mapping[ a hydra:IriTemplateMapping;
                hydra:variable "project"^^xsd:string;
                rdfs:label "Project name";
                rdfs:range "xsd:string" ;
                schema:defaultValue "EPOSMT";
                http:paramValue "EPOSMT";
                http:paramValue "BEAR";
                http:paramValue "SIMBA";
                http:paramValue "MASCA";
                http:paramValue "TOR";
                http:paramValue "EMMA";
                http:paramValue "SVEKA";
                hydra:required "true"^^xsd:boolean;
            ];
            hydra:mapping[ a hydra:IriTemplateMapping;
                hydra:variable "survey"^^xsd:string;
                rdfs:label "Survey name";
                rdfs:range "xsd:string" ;
                schema:defaultValue "2019";
                hydra:required "true"^^xsd:boolean;
            ];
            hydra:mapping[ a hydra:IriTemplateMapping;
                hydra:variable "site"^^xsd:string;
                rdfs:label "Site name";
                rdfs:range "xsd:string" ;
                schema:defaultValue "A01";
                hydra:required "true"^^xsd:boolean;
            ];
    ] ;
.


