# OPERATIONS (#1) => FDSNWS-Event
<https://www.epos-eu.org/epos-dcat-ap/Seismology/Dataset/001/EMSC/operation> a hydra:Operation;
	hydra:method "GET"^^xsd:string;
    #hydra:returns "json, QuakeML";
	hydra:returns "application/xml";
	hydra:property[ a hydra:IriTemplate;
	hydra:template "http://www.seismicportal.eu/fdsnws/event/1/query{?starttime, endtime, minlatitude, maxlatitude, minlongitude, maxlongitude, mindepth, maxdepth, minmagnitude, maxmagnitude, includeallorigins, includearrivals, eventid, limit, offset, orderby, contributor, catalog, updatedafter, format, nodata, latitude, longitude, minradius, maxradius}"^^xsd:string;

		# /!\ Take into account that "includeallmagnitudes" parameters is not included in EMSC FDSNWS-Event... /!\

		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "starttime"^^xsd:string;
			hydra:property "schema:startDate";
			schema:valuePattern "YYYY-MM-DDThh:mm:ss";
			rdfs:range "xsd:dateTime";
			rdfs:label "Start Time";
			schema:minValue "1998-01-01T00:00:00";
			schema:maxValue "3000-12-31T00:00:00";
			schema:defaultValue "2012-01-01T00:00:00";
			hydra:required "false"^^xsd:boolean;
		];

		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "endtime"^^xsd:string;
		    hydra:property "schema:endDate";
			schema:valuePattern "YYYY-MM-DDThh:mm:ss";
			rdfs:range "xsd:dateTime";
			rdfs:label "End Time";
			schema:minValue "1998-01-01T00:00:00";
			schema:maxValue "3000-12-31T00:00:00";
			schema:defaultValue "2019-03-31T00:00:00";
			hydra:required "false"^^xsd:boolean;
		];

		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "minlatitude"^^xsd:string;
			hydra:property "epos:southernmostLatitude";
			rdfs:range "xsd:float";
			rdfs:label "Minimum latitude (deg)";
			schema:minValue "-90";
			schema:maxValue "90";
			schema:defaultValue "30";
			hydra:required "false"^^xsd:boolean;
		];

		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "maxlatitude"^^xsd:string;
			hydra:property "epos:northernmostLatitude";
			rdfs:range "xsd:float";
			rdfs:label "Maximum latitude (deg)";
			schema:minValue "-90";
			schema:maxValue "90";
			schema:defaultValue "90";
			hydra:required "false"^^xsd:boolean;
		];

		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "minlongitude"^^xsd:string;
			hydra:property "epos:westernmostLongitude";
			rdfs:range "xsd:float";
			rdfs:label "Minimum longitude (deg)";
			schema:minValue "-180";
			schema:maxValue "180";
			schema:defaultValue "-40";
			hydra:required "false"^^xsd:boolean;
		];

		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "maxlongitude"^^xsd:string;
			hydra:property "epos:easternmostLongitude";
			rdfs:range "xsd:float";
			rdfs:label "Maximum longitude (deg)";
			schema:minValue "-180";
			schema:maxValue "180";
			schema:defaultValue "50";
			hydra:required "false"^^xsd:boolean;
		];

		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "mindepth"^^xsd:string;
			rdfs:range "xsd:integer";
			rdfs:label "Minimum depth (km)";
			schema:minValue "0";
			schema:maxValue "6371";
			hydra:required "false"^^xsd:boolean;
		];

		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "maxdepth"^^xsd:string;
			rdfs:range "xsd:integer";
			rdfs:label "Maximum depth (km)";
			schema:minValue "0";
			schema:maxValue "6371";
			hydra:required "false"^^xsd:boolean;
		];

		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "minmagnitude"^^xsd:string;
			rdfs:range "xsd:float";
			rdfs:label "Minimum magnitude";
			schema:defaultValue "5";
			hydra:required "false"^^xsd:boolean;
		];

		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "maxmagnitude"^^xsd:string;
			rdfs:range "xsd:float";
			rdfs:label "Maximum magnitude";
			hydra:required "false"^^xsd:boolean;
		];

		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "magnitudetype"^^xsd:string;
			rdfs:range "xsd:string";
			rdfs:label "Type of Magnitude";
			hydra:required "false"^^xsd:boolean;
		];

		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "includeallorigins"^^xsd:string;
			rdfs:range "xsd:boolean";
			rdfs:label "Include All Origins";
			schema:defaultValue "false";
			http:paramValue "true";
			http:paramValue "false";
			hydra:required "false"^^xsd:boolean;
		];

		# Not included in EMSC FDSNWS-Event...
		#hydra:mapping[ a hydra:IriTemplateMapping;
		#	hydra:variable "includeallmagnitudes"^^xsd:string;
		#	rdfs:range "xsd:boolean";
		#	rdfs:label "Include All Magnitudes";
		#	schema:defaultValue "false";
		#	hydra:required "false"^^xsd:boolean;
		#];

		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "includearrivals"^^xsd:string;
			rdfs:range "xsd:boolean";
			rdfs:label "Include All Arrivals";
			schema:defaultValue "false";
			hydra:required "false"^^xsd:boolean;
			http:paramValue "true";
			http:paramValue "false";
		];

		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "eventid"^^xsd:string;
			rdfs:range "xsd:string";
			rdfs:label "Event Identifier";
			hydra:required "false"^^xsd:boolean;
		];

		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "limit"^^xsd:string;
			rdfs:range "xsd:integer";
			rdfs:label "Limit";
			schema:minValue "1";
			hydra:required "false"^^xsd:boolean;
		];

		hydra:mapping[ a hydra:IriTemplateMapping;
